<article class="photo">
  <div class="title">
    <img src="{{photo_url}}" alt={{title}} class="card-img">
  </div>
  <div class="meta">
    {{vote_count}} {{format_plural "like" vote_count}}
    |
    {{comments.length}} {{format_plural "comment" comments.length}}
    <br>
    Posted by {{user.username}} on {{format_date created_at}}
  </div>
</article>